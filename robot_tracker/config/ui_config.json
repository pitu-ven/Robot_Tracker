{
  "window": {
    "title": "Robot Trajectory Controller",
    "width": 1400,
    "height": 900,
    "resizable": true,
    "center_on_startup": true
  },
  "tabs": {
    "camera": {
      "enabled": true,
      "title": "📹 Caméra",
      "tooltip": "Gestion des caméras et acquisition vidéo"
    },
    "target": {
      "enabled": true,
      "title": "🎯 Cible",
      "tooltip": "Détection et tracking des marqueurs"
    },
    "trajectory": {
      "enabled": true,
      "title": "📈 Trajectoire",
      "tooltip": "Analyse des trajectoires et mouvements"
    },
    "calibration": {
      "enabled": true,
      "title": "⚖️ Calibration",
      "tooltip": "Calibration caméra et coordonnées"
    },
    "measures": {
      "enabled": true,
      "title": "📏 Mesures",
      "tooltip": "Outils de mesure et métrologie"
    }
  },
  "camera_tab": {
    "layout": {
      "control_panel_width": 300,
      "video_display_min_height": 400,
      "controls_bar_height": 60,
      "status_bar_height": 25,
      "splitter_sizes": [
        300,
        1000
      ]
    },
    "controls": {
      "buttons": {
        "scan_cameras": "🔍 Scanner",
        "refresh": "🔄 Actualiser",
        "start_stream": "▶️ Démarrer",
        "stop_stream": "⏹️ Arrêter",
        "capture_frame": "📸 Capturer",
        "save_image": "💾 Sauvegarder",
        "settings": "⚙️ Paramètres"
      },
      "tooltips": {
        "scan_cameras": "Scanner les caméras disponibles",
        "refresh": "Actualiser la liste des caméras",
        "start_stream": "Démarrer l'acquisition vidéo",
        "stop_stream": "Arrêter l'acquisition vidéo",
        "capture_frame": "Capturer la frame actuelle",
        "save_image": "Sauvegarder l'image capturée",
        "settings": "Ouvrir les paramètres avancés"
      }
    },
    "video_display": {
      "show_fps": true,
      "show_timestamp": true,
      "show_camera_info": true,
      "default_zoom": 1.0,
      "zoom_step": 0.1,
      "max_zoom": 5.0,
      "min_zoom": 0.1
    },
    "messages": {
      "no_cameras": "Aucune caméra détectée",
      "scanning": "Recherche de caméras en cours...",
      "camera_found": "Caméra détectée: {name}",
      "connection_failed": "Échec de connexion à {name}",
      "streaming_started": "Acquisition démarrée",
      "streaming_stopped": "Acquisition arrêtée",
      "frame_captured": "Frame capturée avec succès",
      "save_success": "Image sauvegardée: {path}",
      "save_failed": "Échec sauvegarde: {error}",
      "opened": "✅ Caméra {name} ouverte",
      "failed": "❌ Échec ouverture {name}",
      "open_error": "Erreur ouverture caméra: {error}",
      "closed": "✅ Caméra {alias} fermée",
      "close_error": "Erreur fermeture {alias}",
      "close_exception": "Erreur fermeture caméra {alias}: {error}",
      "display_added": "🖼️ Affichage {alias} ajouté (vue double: {dual})",
      "display_removed": "🖼️ Affichage {alias} supprimé",
      "starting_stream": "🎬 Démarrage du streaming...",
      "stream_started": "✅ Streaming démarré",
      "start_stream_error": "Erreur démarrage streaming: {error}",
      "stopping_stream": "🛑 Arrêt du streaming...",
      "stream_stopped": "✅ Streaming arrêté",
      "stop_stream_error": "Erreur arrêt streaming: {error}",
      "frame_update_error": "Erreur mise à jour frames: {error}",
      "stats_error": "Erreur mise à jour stats: {error}",
      "refresh_rate": "🔄 Refresh rate: {fps:.1f} FPS",
      "depth_toggled": "👁️ Vue profondeur: {state}",
      "depth_enabled": "Activée",
      "depth_disabled": "Désactivée",
      "camera_clicked": "🖱️ Clic sur caméra: {alias}",
      "no_camera_capture": "⚠️ Aucune caméra sélectionnée pour la capture",
      "capture_failed": "❌ Impossible de capturer une frame de {alias}",
      "capture_error": "Erreur capture frame: {error}",
      "no_camera_save": "⚠️ Aucune caméra sélectionnée pour la sauvegarde",
      "depth_save_success": "💾 Image profondeur sauvegardée: {filepath}",
      "save_error": "Erreur sauvegarde: {filepath}",
      "cleanup": "🔄 Nettoyage terminé"
    },
    "statistics": {
      "columns": [
        "Propriété",
        "Valeur",
        "Unité"
      ],
      "update_interval": 1000,
      "table_max_height": 200,
      "labels": {
        "name": "Nom",
        "type": "Type",
        "resolution": "Résolution",
        "fps": "FPS actuel",
        "frames": "Frames total",
        "timestamp": "Dernière frame",
        "status": "État",
        "depth": "Profondeur"
      },
      "units": {
        "pixels": "pixels",
        "fps": "fps",
        "empty": ""
      },
      "values": {
        "active": "Actif",
        "inactive": "Inactif",
        "na": "N/A"
      }
    },
    "save": {
      "filename_template": "camera_{type}_{timestamp}.jpg",
      "image_formats": "Images (*.jpg *.jpeg *.png);;Tous les fichiers (*)",
      "dialog_title": "Sauvegarder image",
      "depth_suffix": "_depth",
      "depth_extension": ".png"
    },
    "log": {
      "max_height": 120,
      "font_family": "Consolas",
      "font_size": 8,
      "max_lines": 100,
      "timestamp_format": "%H:%M:%S",
      "message_format": "[{timestamp}] {message}"
    },
    "ui_elements": {
      "default_label_font_size": 14,
      "default_label_padding": 50,
      "default_label_border_radius": 10
    }
  },
  "main_window": {
    "about": {
      "status_tip": "Informations sur l'application"
    }
  },
  "camera_manager": {
    "info_docstring": "Informations d'une caméra détectée",
    "streaming": {
      "base_sleep_time": 0.033,
      "poll_failure_thresholds": {
        "high_failure": 10,
        "medium_failure": 5
      },
      "polling_intervals": {
        "problematic": 0.1,
        "medium": 0.05,
        "normal": 0.025
      },
      "log_interval_loops": 300,
      "test_sleep_interval": 0.05,
      "min_fps_threshold": 10,
      "cache_max_age": 0.1,
      "frame_max_age": 0.5,
      "join_timeout": 1.0,
      "error_sleep": 0.1
    }
  },
  "logging": {
    "console_verbosity": "Faible",
    "available_levels": [
      "Faible",
      "Moyenne",
      "Debug"
    ],
    "descriptions": {
      "Faible": "Erreurs et avertissements uniquement",
      "Moyenne": "Informations importantes + erreurs/avertissements",
      "Debug": "Tous les messages de débogage détaillés"
    },
    "file_logging": {
      "enabled": true,
      "log_file": "./logs/robot_tracker.log",
      "max_file_size_mb": 10,
      "backup_count": 3,
      "level": "INFO"
    }
  },
  "theme": {
    "style": "Fusion",
    "palette": "dark",
    "font_family": "Arial",
    "font_size": 10
  }
}